<!-- <app-dashboard-sidemenu></app-dashboard-sidemenu> -->
<p-toast life="50000"></p-toast>
<app-navbar-style-one></app-navbar-style-one>

<section class="hwm-member-form-section">
    <div class="container">
        <div class="heading_section">
            <div class="row m-0">
                <h3 class="m-0 px-0 mb-3">HWM Membership Form</h3>
            </div>
        </div>
        <form (ngSubmit)="Register(register)" #register="ngForm">
            <!-- basic information -->
            <div class="add-listings-box">
                <h3>Basic Informations</h3>
                <div class="row">
                    <div class="col-lg-12 col-md-12">
                        <div class="form-group">
                            <label>Company Name:</label>
                            <input
                                [class.is-invalid]="(register.submitted && company_name?.invalid) || company_name?.invalid && company_name.touched"
                                type="text" class="form-control" name="company_name" #company_name="ngModel"
                                [(ngModel)]="hwmMemberShipFormData.company_name" placeholder="Company Name" required>
                            <div *ngIf="register.submitted || company_name.touched" class="alert alert-danger">
                                <div *ngIf="company_name.errors?.['required']">
                                    Company Name is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>GST Number:</label>
                            <input
                                [class.is-invalid]="(register.submitted && gst_number?.invalid) || gst_number?.invalid && gst_number.touched"
                                type="text" class="form-control" name="gst_number"
                                [(ngModel)]="hwmMemberShipFormData.gst_number" maxlength="15" placeholder="GST Number"
                                #gst_number="ngModel" required>
                            <div *ngIf="register.submitted || gst_number.touched" class="alert alert-danger">
                                <div *ngIf="gst_number.errors?.['required']">
                                    GST Number is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>GST Type:</label>
                            <input
                                [class.is-invalid]="(register.submitted && gst_type?.invalid) || gst_type?.invalid && gst_type.touched"
                                type="text" class="form-control" name="gst_type"
                                [(ngModel)]="hwmMemberShipFormData.gst_type" placeholder="GST Type" #gst_type="ngModel"
                                required>
                            <div *ngIf="register.submitted || gst_type.touched" class="alert alert-danger">
                                <div *ngIf="gst_type.errors?.['required']">
                                    GST Type is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Party Type:</label>
                            <input
                                [class.is-invalid]="(register.submitted && party_type?.invalid) || party_type?.invalid && party_type.touched"
                                type="text" class="form-control" name="party_type"
                                [(ngModel)]="hwmMemberShipFormData.party_type" placeholder="Party Type"
                                #party_type="ngModel" required>
                            <div *ngIf="register.submitted || party_type.touched" class="alert alert-danger">
                                <div *ngIf="party_type.errors?.['required']">
                                    Party Type is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>CIN Number:</label>
                            <input
                                [class.is-invalid]="(register.submitted && cin_number?.invalid) || cin_number?.invalid && cin_number.touched"
                                placeholder="CIN Number" type="text" [(ngModel)]="hwmMemberShipFormData.cin_number"
                                class="form-control" name="cin_number" #cin_number="ngModel" required>
                            <div *ngIf="register.submitted || cin_number.touched" class="alert alert-danger">
                                <div *ngIf="cin_number.errors?.['required']">
                                    CIN Number is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>UIN Number:</label>
                            <input
                                [class.is-invalid]="(register.submitted && uin_number?.invalid) || uin_number?.invalid && uin_number.touched"
                                placeholder="UIN Number" type="text" [(ngModel)]="hwmMemberShipFormData.uin_number"
                                class="form-control" name="uin_number" #uin_number="ngModel" required>
                            <div *ngIf="register.submitted || uin_number.touched" class="alert alert-danger">
                                <div *ngIf="uin_number.errors?.['required']">
                                    UIN Number is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Average Fulltime
                                Employee:</label>
                            <input
                                [class.is-invalid]="(register.submitted && avg_fulltime_emp?.invalid) || avg_fulltime_emp?.invalid && avg_fulltime_emp.touched"
                                type="text" name="avg_fulltime_emp" class="form-control"
                                [(ngModel)]="hwmMemberShipFormData.avg_fulltime_emp"
                                placeholder="Average Fulltime Employee" #avg_fulltime_emp="ngModel" required>
                            <div *ngIf="register.submitted || avg_fulltime_emp.touched" class="alert alert-danger">
                                <div *ngIf="avg_fulltime_emp.errors?.['required']">
                                    Field is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Office Telephone:</label>
                            <input
                                [class.is-invalid]="(register.submitted && office_telephone?.invalid) || office_telephone?.invalid && office_telephone.touched"
                                type="text" class="form-control" name="office_telephone" maxlength="10"
                                [(ngModel)]="hwmMemberShipFormData.office_telephone" pattern="^((\\+91-?)|0)?[0-9]{10}$"
                                placeholder="Office Telephone" #office_telephone="ngModel" required>
                            <div *ngIf="register.submitted || office_telephone.touched" class="alert alert-danger">
                                <div *ngIf="office_telephone.errors?.['required']">
                                    Office Telephone is required.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Residental
                                Telephone:</label>
                            <input
                                [class.is-invalid]="(register.submitted && residential_telephone?.invalid) || residential_telephone?.invalid && residential_telephone.touched"
                                type="text" class="form-control"
                                [(ngModel)]="hwmMemberShipFormData.residential_telephone" maxlength="10"
                                pattern="^((\\+91-?)|0)?[0-9]{10}$" name="residential_telephone"
                                placeholder="Residental Telephone" #residential_telephone="ngModel" required>
                            <div *ngIf="register.submitted || residential_telephone.touched" class="alert alert-danger">
                                <div *ngIf="residential_telephone.errors?.['required']">
                                    Residental Telephone is required.
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- address section -->
            <div class="add-listings-box">
                <div class="row no-gutters">
                    <div class="col-lg-6 col-md-6 col-12">
                        <h3>Residential Address</h3>
                    </div>
                    <div class="col-lg-6 col-md-6 col-12">
                        <h3>Factory Address (Optional)</h3>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-12">
                        <div class="row no-gutters">
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label>Address:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && address?.invalid) || address?.invalid && address.touched"
                                        type="text" class="form-control" name="address"
                                        [(ngModel)]="hwmMemberShipFormData.address" placeholder="e.g. 55 County Laois"
                                        #address="ngModel" required>
                                    <div *ngIf="register.submitted || address.touched" class="alert alert-danger">
                                        <div *ngIf="address.errors?.['required']">
                                            Address is required.
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label>State:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && state?.invalid) || state?.invalid && state.touched"
                                        placeholder="State" type="text" [(ngModel)]="hwmMemberShipFormData.state"
                                        class="form-control" name="state" #state="ngModel" required>
                                    <div *ngIf="register.submitted || state.touched" class="alert alert-danger">
                                        <div *ngIf="state.errors?.['required']">
                                            State is required.
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label> City:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && city?.invalid) || city?.invalid && city.touched"
                                        type="text" class="form-control" [(ngModel)]="hwmMemberShipFormData.city"
                                        name="city" placeholder="City" #city="ngModel" required>
                                    <div *ngIf="register.submitted || city.touched" class="alert alert-danger">
                                        <div *ngIf="city.errors?.['required']">
                                            City is required.
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label>Zip Code:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && pin_code?.invalid) || pin_code?.invalid && pin_code.touched"
                                        placeholder="Zip Code" type="text" [(ngModel)]="hwmMemberShipFormData.pin_code"
                                        class="form-control" name="pin_code" #pin_code="ngModel" required>
                                    <div *ngIf="register.submitted || pin_code.touched" class="alert alert-danger">
                                        <div *ngIf="pin_code.errors?.['required']">
                                            Zip Code is required.
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-12">
                        <div class="row no-gutters">
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label>Factory Address:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && factory_address?.invalid) || factory_address?.invalid && factory_address.touched"
                                        type="text" class="form-control"
                                        [(ngModel)]="hwmMemberShipFormData.factory_address" name="factory_address"
                                        placeholder="e.g. 55 County Laois" #factory_address="ngModel" required>
                                    <div *ngIf="register.submitted || factory_address.touched"
                                        class="alert alert-danger">
                                        <div *ngIf="factory_address.errors?.['required']">
                                            Factory Address is required.
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label>Factory City:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && factory_city?.invalid) || factory_city?.invalid && factory_city.touched"
                                        type="text" class="form-control"
                                        [(ngModel)]="hwmMemberShipFormData.factory_city" name="factory_city"
                                        placeholder="Factory City" #factory_city="ngModel" required>
                                    <div *ngIf="register.submitted || factory_city.touched" class="alert alert-danger">
                                        <div *ngIf="factory_city.errors?.['required']">
                                            Factory City is required.
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label>Factory State:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && factory_state?.invalid) || factory_state?.invalid && factory_state.touched"
                                        placeholder="Factory State" type="text"
                                        [(ngModel)]="hwmMemberShipFormData.factory_state" class="form-control"
                                        name="factory_state" #factory_state="ngModel" required>
                                    <div *ngIf="register.submitted || factory_state.touched" class="alert alert-danger">
                                        <div *ngIf="factory_state.errors?.['required']">
                                            Factory State is required.
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label>Factory Zip Code:</label>
                                    <input
                                        [class.is-invalid]="(register.submitted && factory_pin_code?.invalid) || factory_pin_code?.invalid && factory_pin_code.touched"
                                        placeholder="Factory Zip Code" type="text"
                                        [(ngModel)]="hwmMemberShipFormData.factory_pin_code" class="form-control"
                                        name="factory_pin_code" #factory_pin_code="ngModel" required>
                                    <div *ngIf="register.submitted || factory_pin_code.touched"
                                        class="alert alert-danger">
                                        <div *ngIf="factory_pin_code.errors?.['required']">
                                            Factory Zip Code is required.
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- company details -->
            <div class="add-listings-box">
                <h3>Company Details</h3>

                <div class="row">
                    <div class="col-lg-12 col-md-12">
                        <div class="form-group">
                            <label>Registration
                                Number:</label>
                            <input
                                [class.is-invalid]="(register.submitted && registration_number?.invalid) || registration_number?.invalid && registration_number.touched"
                                type="text" name="registration_number" class="form-control"
                                [(ngModel)]="hwmMemberShipFormData.registration_number"
                                placeholder="Registration Number" #registration_number="ngModel" required>
                            <div *ngIf="register.submitted || registration_number.touched" class="alert alert-danger">
                                <div *ngIf="registration_number.errors?.['required']">
                                    Registration Number is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Capital
                                Investment:</label>
                            <input
                                [class.is-invalid]="(register.submitted && capital_investment?.invalid) || capital_investment?.invalid && capital_investment.touched"
                                type="text" placeholder="Capital Investment" class="form-control"
                                [(ngModel)]="hwmMemberShipFormData.capital_investment" name="capital_investment"
                                #capital_investment="ngModel" required>
                            <div *ngIf="register.submitted || capital_investment.touched" class="alert alert-danger">
                                <div *ngIf="capital_investment.errors?.['required']">
                                    Capital Investment is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Website:</label>
                            <input
                                [class.is-invalid]="(register.submitted && website?.invalid) || website?.invalid && website.touched"
                                type="text" class="form-control" placeholder="Website"
                                [(ngModel)]="hwmMemberShipFormData.website" name="website" #website="ngModel" required>
                            <div *ngIf="register.submitted || website.touched" class="alert alert-danger">
                                <div *ngIf="website.errors?.['required']">
                                    Website is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>E-Mail:</label>
                            <input
                                [class.is-invalid]="(register.submitted && email?.invalid) || email?.invalid && email.touched"
                                type="email" class="form-control" placeholder="Email Address" email
                                [(ngModel)]="hwmMemberShipFormData.email" name="email" #email="ngModel" required>
                            <div *ngIf="register.submitted || email.touched" class="alert alert-danger">
                                <div *ngIf="email.errors?.['required']">
                                    E-Mail is required.
                                </div>
                                <div *ngIf="email.errors?.email">
                                    E-Mail is not valid.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Nature of Business:</label>
                            <input
                                [class.is-invalid]="(register.submitted && nature_of_business?.invalid) || nature_of_business?.invalid && nature_of_business.touched"
                                type="text" placeholder="Nature of Business"
                                [(ngModel)]="hwmMemberShipFormData.nature_of_business" name="nature_of_business"
                                class="form-control" #nature_of_business="ngModel" required>
                            <div *ngIf="register.submitted || nature_of_business.touched" class="alert alert-danger">
                                <div *ngIf="nature_of_business.errors?.['required']">
                                    Field is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>About:</label>
                            <input
                                [class.is-invalid]="(register.submitted && about?.invalid) || about?.invalid && about.touched"
                                type="text" class="form-control" placeholder="About"
                                [(ngModel)]="hwmMemberShipFormData.about" name="about" #about="ngModel" required>
                            <div *ngIf="register.submitted || about.touched" class="alert alert-danger">
                                <div *ngIf="about.errors?.['required']">
                                    Field is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="file">Upload Logo:</label>
                            <input
                                [class.is-invalid]="(register.submitted && logo?.invalid) || logo?.invalid && logo.touched"
                                type="file" name="logo" accept="image/x-png,image/gif,image/jpeg"
                                (change)="fileUpload($event,'logo')" [(ngModel)]="hwmMemberShipFormData.logo"
                                id="logofile" class="inputfile" #logo="ngModel" required />
                            <div *ngIf="register.submitted || logo.touched" class="alert alert-danger">
                                <div *ngIf="logo.errors?.['required']">
                                    Logo is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Memeber Classfication:</label>
                            <!-- <input [class.is-invalid]="(register.submitted && company_name?.invalid) || company_name?.invalid && company_name.touched" type="text" class="form-control"
                                placeholder="Memeber Classfication"
                                name="memberclassficationid"> -->
                            <select name="member_classification_id"
                                [class.is-invalid]="(register.submitted && member_classification_id?.invalid) || member_classification_id?.invalid && member_classification_id.touched"
                                [(ngModel)]="hwmMemberShipFormData.member_classification_id"
                                class="selectpicker dropdown form-control" id="type" data-live-search="true"
                                #member_classification_id="ngModel" required>
                                <option [ngValue]="null" disabled>Please select
                                    member
                                    classfication</option>
                                <option *ngFor="let item of member_classification.data" [ngValue]="item.id">
                                    {{item.name}}</option>
                            </select>
                            <div *ngIf="register.submitted || member_classification_id.touched"
                                class="alert alert-danger">
                                <div *ngIf="member_classification_id.errors?.['required']">
                                    Memeber Classfication is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Memeber Category:</label>
                            <select name="member_category_id" [(ngModel)]="hwmMemberShipFormData.member_category_id"
                                [class.is-invalid]="(register.submitted && member_category_id?.invalid) || member_category_id?.invalid && member_category_id.touched"
                                class="selectpicker dropdown form-control" id="type" data-live-search="true"
                                #member_category_id="ngModel" required>
                                <option [ngValue]="null" disabled>Please select
                                    member
                                    category</option>
                                <option *ngFor="let item of categoryData.data" [ngValue]="item.id">{{item.name}}
                                </option>
                            </select>
                            <div *ngIf="register.submitted || member_category_id.touched" class="alert alert-danger">
                                <div *ngIf="member_category_id.errors?.['required']">
                                    Memeber Category is required.
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- nominee one details -->
            <div class="add-listings-box">
                <h3>HWM Owner Details</h3>
                <div class="row">

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="">Name:</label>
                            <input
                                [class.is-invalid]="(register.submitted && name1?.invalid) || name1?.invalid && name1.touched"
                                type="text" class="form-control" placeholder="Name"
                                [(ngModel)]="hwmMemberShipFormData.name1" name="name1" #name1="ngModel" required>
                            <div *ngIf="register.submitted || name1.touched" class="alert alert-danger">
                                <div *ngIf="name1.errors?.['required']">
                                    Name is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="">Designation:</label>
                            <input
                                [class.is-invalid]="(register.submitted && designation1?.invalid) || designation1?.invalid && designation1.touched"
                                type="text" class="form-control" placeholder="Designation"
                                [(ngModel)]="hwmMemberShipFormData.designation1" name="designation1"
                                #designation1="ngModel" required>
                            <div *ngIf="register.submitted || designation1.touched" class="alert alert-danger">
                                <div *ngIf="designation1.errors?.['required']">
                                    Designation is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="">Date of Birth:</label>
                            <p-calendar name="date_of_birth1" showClear="true"
                                [(ngModel)]="hwmMemberShipFormData.date_of_birth1" [readonlyInput]="true"
                                placeholder="Please Select Date : YYYY/MM/DD" [showIcon]="true"
                                #date_of_birth1="ngModel" required></p-calendar>
                            <div *ngIf="register.submitted || date_of_birth1.touched" class="alert alert-danger">
                                <div *ngIf="date_of_birth1.errors?.['required']">
                                    DOB is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="">E-Mail</label>
                            <input
                                [class.is-invalid]="(register.submitted && email1?.invalid) || email1?.invalid && email1.touched"
                                type="email" class="form-control" placeholder="E-Mail"
                                [(ngModel)]="hwmMemberShipFormData.email1" email name="email1" #email1="ngModel"
                                required>
                            <div *ngIf="register.submitted || email1.touched" class="alert alert-danger">
                                <div *ngIf="email1.errors?.['required']">
                                    E-Mail is required.
                                </div>
                                <div *ngIf="email1.errors?.email">
                                    E-Mail is not valid.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Office Phone:</label>
                            <input
                                [class.is-invalid]="(register.submitted && office_phone1?.invalid) || office_phone1?.invalid && office_phone1.touched"
                                type="text" name="office_phone1" class="form-control"
                                [(ngModel)]="hwmMemberShipFormData.office_phone1" maxlength="10"
                                placeholder="Office Phone" #office_phone1="ngModel" required>
                            <div *ngIf="register.submitted || office_phone1.touched" class="alert alert-danger">
                                <div *ngIf="office_phone1.errors?.['required']">
                                    Office Phone is required.
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Mobile Number:</label>
                            <input
