                            <div *ngIf="register.submitted &&
                                f.name1.errors?.required ||
                                f.name1.touched" class="alert alert-danger">
                                <div *ngIf="f.name1.errors?.required">
                                    Name is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="">Designation:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('designation1')?.invalid) || registerForm.get('designation1')?.invalid && registerForm.get('designation1').touched"  type="text" class="form-control" placeholder="Designation"
                                formControlName="designation1">
                            <div *ngIf="register.submitted &&
