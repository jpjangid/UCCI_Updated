                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="">Date of Birth:</label>
                            <p-calendar formControlName="dob1" showClear="true"
                                placeholder="Please Select Date : YYYY/MM/DD" [showIcon]="true" readonly></p-calendar>
                            <!-- <input [class.is-invalid]="(register.submitted && registerForm.get('companyname')?.invalid) || registerForm.get('companyname')?.invalid && registerForm.get('companyname').touched"  type="date" class="form-control"
                                placeholder="Designation"
                                formControlName="dob1"> -->
                            <div *ngIf="register.submitted &&
