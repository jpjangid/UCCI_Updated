<!-- regular membership form section -->


<section class="regular-member-form-section">
    <div class="container">
        <div class="heading_section">
            <div class="row m-0">
                <h3 class="m-0 px-0 mb-3">Regular Membership Form</h3>
            </div>
        </div>
        <form [formGroup]="registerForm" (ngSubmit)="Register(register)" #register="ngForm">
            <!-- basic information -->
            <div class="add-listings-box">
                <h3>Basic Informations</h3>
                <div class="row">
                    <div class="col-lg-12 col-md-12">
                        <div class="form-group">
                            <label>Company
                                Name:</label>
                            <input type="text" class="form-control" formControlName="companyname"
                                placeholder="Company Name"
                                [class.is-invalid]="(register.submitted && registerForm.get('companyname')?.invalid) || registerForm.get('companyname')?.invalid && registerForm.get('companyname').touched">
                            <div *ngIf="register.submitted &&
                                f.companyname.errors?.required ||
                                f.companyname.touched" class="alert alert-danger">
                                <div *ngIf="f.companyname.errors?.required">
                                    Company Name is required
                                </div>
                                <!-- <div *ngIf="f.companyname.errors?.pattern">
                                    Company Name
                                    is
                                    invalid
                                </div> -->
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>GST Number:</label>
                            <input type="text" class="form-control" formControlName="gstno" maxlength="15"
                                (keydown)="bloackAlphabet($event)" pattern="^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$"
                                [class.is-invalid]="(register.submitted && registerForm.get('gstno')?.invalid) || registerForm.get('gstno')?.invalid && registerForm.get('gstno').touched"
                                placeholder="GST Number">
                            <div *ngIf="register.submitted &&
                                f.gstno.errors?.required ||
                                f.gstno.touched" class="alert alert-danger">
                                <div *ngIf="f.gstno.errors?.required">
                                    GST Number is required
                                </div>
                                <div *ngIf="f.gstno.errors?.pattern">
                                    Please enter 15 digits GST number
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Average Fulltime
                                Employee:</label>
                            <input type="text" formControlName="avgemp" class="form-control"
                                (keydown)="bloackAlphabet($event)" placeholder="Average Fulltime Employee"
                                [class.is-invalid]="(register.submitted && registerForm.get('avgemp')?.invalid) || registerForm.get('avgemp')?.invalid && registerForm.get('avgemp').touched">
                            <div *ngIf="register.submitted &&
                                f.avgemp.errors?.required ||
                                f.avgemp.touched" class="alert alert-danger">
                                <div *ngIf="f.avgemp.errors?.required">
                                    Average Fulltime Employee is required
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Office Telephone:</label>
                            <input type="text" class="form-control" formControlName="telephone" maxlength="10"
                                (keydown)="bloackAlphabet($event)" pattern="^((\\+91-?)|0)?[0-9]{10}$" [class.is-invalid]="(register.submitted && registerForm.get('telephone')?.invalid) || registerForm.get('telephone')?.invalid && registerForm.get('telephone').touched"
                                placeholder="Office Telephone">
                            <div *ngIf="register.submitted &&
                                f.telephone.errors?.required ||
                                f.telephone.touched" class="alert alert-danger">
                                <div *ngIf="f.telephone.errors?.required">
                                    Office Telephone is required
                                </div>
                                <div *ngIf="f.telephone.errors?.pattern">
                                    Please enter 10 digits office telephone number
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Residental
                                Telephone:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('residentaltel')?.invalid) || registerForm.get('residentaltel')?.invalid && registerForm.get('residentaltel').touched"  type="text" class="form-control" (keydown)="bloackAlphabet($event)" maxlength="10"
                                pattern="^((\\+91-?)|0)?[0-9]{10}$" formControlName="residentaltel"
                                placeholder="Residental Telephone">
                            <div *ngIf="register.submitted &&
                                f.residentaltel.errors?.required ||
                                f.residentaltel.touched" class="alert alert-danger">
                                <div *ngIf="f.residentaltel.errors?.required">
                                    Residental Telephone is required
                                </div>
                                <div *ngIf="f.residentaltel.errors?.pattern">
                                    Please enter 10 digits residental telephone number
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- address section -->
            <div class="add-listings-box">
                <h3>Location</h3>
                <div class="row">
                    <div class="col-lg-12 col-md-12">
                        <div class="form-group">
                            <label>Address:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('address')?.invalid) || registerForm.get('address')?.invalid && registerForm.get('address').touched"  type="text" class="form-control" formControlName="address"
                                placeholder="e.g. 55 County Laois">
                            <div *ngIf="register.submitted &&
                                f.address.errors?.required ||
                                f.address.touched" class="alert alert-danger">
                                <div *ngIf="f.address.errors?.required">
                                    Address is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>State:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('state')?.invalid) || registerForm.get('state')?.invalid && registerForm.get('state').touched"  placeholder="State" type="text" class="form-control" formControlName="state">
                            <div *ngIf="register.submitted &&
                                f.state.errors?.required ||
                                f.state.touched" class="alert alert-danger">
                                <div *ngIf="f.state.errors?.required">
                                    State is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label> City:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('city')?.invalid) || registerForm.get('city')?.invalid && registerForm.get('city').touched"  type="text" class="form-control" formControlName="city" placeholder="City">
                            <div *ngIf="register.submitted &&
                                f.city.errors?.required ||
                                f.city.touched" class="alert alert-danger">
                                <div *ngIf="f.city.errors?.required">
                                    City is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Zip-Code:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('zipcode')?.invalid) || registerForm.get('zipcode')?.invalid && registerForm.get('zipcode').touched"  placeholder="Zip-Code" type="text" (keydown)="bloackAlphabet($event)"
                                class="form-control" formControlName="zipcode">
                            <div *ngIf="register.submitted &&
                                f.zipcode.errors?.required ||
                                f.zipcode.touched" class="alert alert-danger">
                                <div *ngIf="f.zipcode.errors?.required">
                                    Zip-Code is required
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- company details -->
            <div class="add-listings-box">
                <h3>Company Details</h3>

                <div class="row">
                    <div class="col-lg-12 col-md-12">
                        <div class="form-group">
                            <label>Registration
                                Number:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('registrationnumber')?.invalid) || registerForm.get('registrationnumber')?.invalid && registerForm.get('registrationnumber').touched"  type="text" formControlName="registrationnumber" class="form-control"
                                (keydown)="bloackAlphabet($event)" placeholder="Registration Number">
                            <div *ngIf="register.submitted &&
                                f.registrationnumber.errors?.required ||
                                f.registrationnumber.touched" class="alert alert-danger">
                                <div *ngIf="f.registrationnumber.errors?.required">
                                    Registration Number is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Capital
                                Investment:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('capitalinvestment')?.invalid) || registerForm.get('capitalinvestment')?.invalid && registerForm.get('capitalinvestment').touched"  type="text" placeholder="Capital Investment" class="form-control"
                                (keydown)="bloackAlphabet($event)" formControlName="capitalinvestment">
                            <div *ngIf="register.submitted &&
                                f.capitalinvestment.errors?.required ||
                                f.capitalinvestment.touched" class="alert alert-danger">
                                <div *ngIf="f.capitalinvestment.errors?.required">
                                    Capital Investment is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Website:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('website')?.invalid) || registerForm.get('website')?.invalid && registerForm.get('website').touched"  type="text" class="form-control" placeholder="Website" formControlName="website">
                            <div *ngIf="register.submitted &&
                                f.website.errors?.required ||
                                f.website.touched" class="alert alert-danger">
                                <div *ngIf="f.website.errors?.required">
                                    Website is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Email Address:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('email')?.invalid) || registerForm.get('email')?.invalid && registerForm.get('email').touched"  type="email" class="form-control" placeholder="Email Address"
                                pattern="^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$" formControlName="email">
                            <div *ngIf="register.submitted &&
                                f.email.errors?.required ||
                                f.email.touched" class="alert alert-danger">
                                <div *ngIf="f.email.errors?.required">
                                    Emaill is required
                                </div>
                                <div *ngIf="f.email.errors?.pattern">
                                    Invalid emaill address
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Nature of Business:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('businessnature')?.invalid) || registerForm.get('businessnature')?.invalid && registerForm.get('businessnature').touched"  type="text" placeholder="Nature of Business" formControlName="businessnature"
                                class="form-control">
                            <div *ngIf="register.submitted &&
                                f.businessnature.errors?.required ||
                                f.businessnature.touched" class="alert alert-danger">
                                <div *ngIf="f.businessnature.errors?.required">
                                    Nature of Business is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>About:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('about')?.invalid) || registerForm.get('about')?.invalid && registerForm.get('about').touched"  type="text" class="form-control" placeholder="About" formControlName="about">
                            <div *ngIf="register.submitted &&
                                f.about.errors?.required ||
                                f.about.touched" class="alert alert-danger">
                                <div *ngIf="f.about.errors?.required">
                                    About is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="file">Upload Logo</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('logo')?.invalid) || registerForm.get('complogoanyname')?.invalid && registerForm.get('logo').touched"  type="file" formControlName="logo" accept="image/x-png,image/gif,image/jpeg"
                                name="logofile" id="logofile" (change)="processImage($event, 'logo')"
                                class="inputfile" />
                            <div *ngIf="register.submitted &&
                                f.logo.errors?.required ||
                                f.logo.touched" class="alert alert-danger">
                                <div *ngIf="f.logo.errors?.required">
                                    Logo is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Memeber Classfication:</label>
                            <!-- <input [class.is-invalid]="(register.submitted && registerForm.get('companyname')?.invalid) || registerForm.get('companyname')?.invalid && registerForm.get('companyname').touched"  type="text" class="form-control"
                                placeholder="Memeber Classfication"
                                formControlName="memberclassficationid"> -->
                            <select [class.is-invalid]="(register.submitted && registerForm.get('memberclassficationid')?.invalid) || registerForm.get('memberclassficationid')?.invalid && registerForm.get('memberclassficationid').touched"  formControlName="memberclassficationid" class="selectpicker dropdown form-control" id="type"
                                data-live-search="true">
                                <option [ngValue]="null" disabled>Please select
                                    member
                                    classfication</option>
                                <option *ngFor="let item of
                                    classficationData.data" [ngValue]="item.id">{{item.name}}</option>
                            </select>
                            <div *ngIf="register.submitted &&
                                f.memberclassficationid.errors?.required ||
                                f.memberclassficationid.touched" class="alert alert-danger">
                                <div *ngIf="f.memberclassficationid.errors?.required">
                                    Memeber Classfication is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Memeber Category:</label>
                            <!-- <input [class.is-invalid]="(register.submitted && registerForm.get('companyname')?.invalid) || registerForm.get('companyname')?.invalid && registerForm.get('companyname').touched"  type="text" class="form-control"
                                placeholder="Memeber Category"
                                formControlName="membercategoryid"> -->
                            <select [class.is-invalid]="(register.submitted && registerForm.get('membercategoryid')?.invalid) || registerForm.get('membercategoryid')?.invalid && registerForm.get('membercategoryid').touched"  formControlName="membercategoryid" class="selectpicker dropdown form-control" id="type"
                                data-live-search="true">
                                <option [ngValue]="null" disabled>Please select
                                    member
                                    category</option>
                                <option *ngFor="let item of
                                    categoryData.data" [ngValue]="item.id">{{item.name}}</option>
                            </select>
                            <div *ngIf="register.submitted &&
                                f.membercategoryid.errors?.required ||
                                f.membercategoryid.touched" class="alert alert-danger">
                                <div *ngIf="f.membercategoryid.errors?.required">
                                    Memeber Category is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Applicant Constitution:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('applicantconstitution')?.invalid) || registerForm.get('applicantconstitution')?.invalid && registerForm.get('applicantconstitution').touched"  type="text" class="form-control" placeholder="Applicant Constitution"
                                formControlName="applicantconstitution">
                            <div *ngIf="register.submitted &&
                                f.applicantconstitution.errors?.required ||
                                f.applicantconstitution.touched" class="alert alert-danger">
                                <div *ngIf="f.applicantconstitution.errors?.required">
                                    Applicant Constitution is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Establishment Date:</label>
                            <p-calendar formControlName="establishdate" showClear="true"
                                placeholder="Please Select Date : YYYY/MM/DD" [showIcon]="true" readonly></p-calendar>
                            <!-- <input [class.is-invalid]="(register.submitted && registerForm.get('companyname')?.invalid) || registerForm.get('companyname')?.invalid && registerForm.get('companyname').touched"  type="date" class="form-control"
                                placeholder="Establishment Date"
                                formControlName="establishdate"> -->
                            <div *ngIf="register.submitted &&
                                f.establishdate.errors?.required ||
                                f.establishdate.touched" class="alert alert-danger">
                                <div *ngIf="f.establishdate.errors?.required">
                                    Establishment Date is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Turnover:</label>
                            <select [class.is-invalid]="(register.submitted && registerForm.get('turnoverid')?.invalid) || registerForm.get('turnoverid')?.invalid && registerForm.get('turnoverid').touched"  formControlName="turnoverid" class="selectpicker dropdown form-control" id="type"
                                data-live-search="true">
                                <option [ngValue]="null" disabled>Please select
                                    turnover</option>
                                <option *ngFor="let item of
                                    turnoverData.data" [ngValue]="item.id">{{item.component}}</option>
                            </select>
                            <!-- <input [class.is-invalid]="(register.submitted && registerForm.get('companyname')?.invalid) || registerForm.get('companyname')?.invalid && registerForm.get('companyname').touched"  type="text" class="form-control"
                                placeholder="Turnover Id"
                                formControlName="turnoverid"> -->
                            <div *ngIf="register.submitted &&
                                f.turnoverid.errors?.required ||
                                f.turnoverid.touched" class="alert alert-danger">
                                <div *ngIf="f.turnoverid.errors?.required">
                                    Turnover Id is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Registered By:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('registerby')?.invalid) || registerForm.get('registerby')?.invalid && registerForm.get('registerby').touched"  type="text" class="form-control" placeholder="Registered By"
                                formControlName="registerby">
                            <div *ngIf="register.submitted &&
                                f.registerby.errors?.required ||
                                f.registerby.touched" class="alert alert-danger">
                                <div *ngIf="f.logo.errors?.required">
                                    Registered By is required
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label>Registered By:</label>
                            <input [class.is-invalid]="(register.submitted && registerForm.get('companyname')?.invalid) || registerForm.get('companyname')?.invalid && registerForm.get('companyname').touched"  type="text" class="form-control"
                                placeholder="Registered By"
                                formControlName="registerby">
                        </div>
                    </div> -->
                </div>
            </div>
            <!-- nominee one details -->
            <div class="add-listings-box">
                <h3>Primary Nominee Details</h3>
                <div class="row">

                    <div class="col-lg-4 col-md-6">
                        <div class="form-group">
                            <label for="">Name:</label>
